{
  "name": "fitwell",
  "version": "1.1.2",
  "description": "Personal fitness tracking app for Justin & Aimee",
  "main": "dist/main/main.js",
  "type": "commonjs",
  "author": "Justin McGill",
  "license": "MIT",
  "scripts": {
    "postinstall": "node scripts/patch-electron-name.js",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "build": "tsc -p tsconfig.main.json && vite build && electron-builder",
    "build:vite": "vite build",
    "build:electron": "tsc -p tsconfig.main.json",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "date-fns": "^4.1.0",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.6.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^11.0.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.1",
    "electron": "^33.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.17.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "com.fitwell.app",
    "productName": "FitWell",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "owner": "JJMcGil1",
      "repo": "FitWell"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
